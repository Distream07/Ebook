{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/css/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "public/js/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/ebooks",
        "dest": "/server.js"
      },
      {
        "src": "/api/ebooks/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  