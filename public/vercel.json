{
    "version": 2,
    "builds": [
      {
        "src": "../server.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "css/styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "js/script.js",
        "use": "@vercel/static"
      },
      {
        "src": "js/*",
        "use": "@vercel/static"
      },
      {
        "src": "css/*",
        "use": "@vercel/static"
      },
      {
        "src": "image/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
    {
      "src": "/api/ebooks",
      "dest": "/server.js"
    },
    {
      "src": "/api/ebooks/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
  }
  